{
  "output": {
    "filePath": "repomix-output.txt",
    "style": "plain",
    "parsableStyle": true,
    "compress": false,
    "fileSummary": false,
    "directoryStructure": true,
    "removeComments": true,
    "removeEmptyLines": true,
    "showLineNumbers": false,
    "copyToClipboard": false,
    "includeEmptyDirectories": false
  },
  "include": ["**/*"],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "tmp/",
      "*.log",
      "components/ui/",
      "tailwind.config.ts",
      "yarn.lock",
      "tsconfig.json",
      "components.json",
      "bun.lockb",
      "postcss.config.mjs",
      "next.config.ts",
      "README.md",
      "*.svg",
      "public/*",
      "repomix.config.json",
      "package.json",
      "components/admin/admin-top-nav.tsx",
      "components/admin/entity/*.tsx",
      "components/admin/admin/*.tsx",
      "components/admin/users/*.tsx",
      "components/admin/logs/*.tsx",
      "components/admin/projects/*.tsx",
      "components/admin/projects/project-detail/*.tsx",
      "components/dashboard/project-detail/*.tsx",
      "components/dashboard/*.tsx",
      "app/admin/admins/page.tsx",
      "app/admin/layout.tsx",
      "app/admin/logs/page.tsx",
      "app/admin/page.tsx",
      "app/admin/users/page.tsx",
      "app/login/page.tsx",
      "app/dashboard/layout.tsx",
      "app/dashboard/page.tsx",
      "app/admin/projects/*.tsx",
      "components/auth/login-form.tsx",
      "eslint.config.mjs",
      ".gitignore",
      "next-env.d.ts",
      ".env.local",
      "app/globals.css",
      "app/layout.tsx",
      "app/page.tsx"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  }
}
